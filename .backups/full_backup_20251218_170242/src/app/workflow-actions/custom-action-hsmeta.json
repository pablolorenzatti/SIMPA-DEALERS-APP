    "labels": {
      "en": {
        "actionName": "Configure External Account Properties",
        "actionDescription": "Creates/updates CRM properties and pipelines by brand/dealer",
        "appDisplayName": "SIMPA-Application",
        "actionCardContent": "Setup for {{marca}} / {{dealer}}",
        "inputFieldLabels": {
          "marca": "Brand",
          "modelo": "Model",
          "dealer": "Dealer",
          "razon_social": "Legal Entity",
          "tipo_proceso": "Process Type"
        },
        "inputFieldDescriptions": {
          "marca": "Brand that drives property configuration. Required for 'Create/Update Model Properties', 'Create All Properties', and 'Create/Configure Business Pipeline' processes.",
          "modelo": "Model to add/update in the properties. Only required for 'Create/Update Model Properties' process.",
          "dealer": "Dealer that drives property configuration. Required for 'Create/Update Dealership Properties' and 'Create/Configure Business Pipeline' processes. Optional for 'Create All Properties'.",
          "razon_social": "Legal entity configured with a secret to resolve tokens automatically. Required for all process types EXCEPT 'Create All Properties (Global)'. Leave empty for bulk model updates or global process.",
          "tipo_proceso": "Select what should be configured"
        },
        "outputFieldLabels": {
          "success": "Success",
          "message": "Message",
          "razonSocial": "Legal Entity",
          "propertiesCreated": "Properties Created",
          "propertiesSkipped": "Properties Skipped",
          "propertiesUpdated": "Properties Updated",
          "optionsAdded": "Options Added",
          "tipoProceso": "Executed Process Type",
          "updatedPropertiesList": "Updated Properties",
          "optionDetails": "Option Details",
