          "bulkErroresCount": "Bulk Errors Count",
          "bulkErroresDetalle": "Bulk Errors Detail",
          "bulkExitosasDetalle": "Bulk Successful Detail"
        }
      },
      "es": {
        "actionName": "Configurar Propiedades de Cuenta Externa",
        "actionDescription": "Crea/actualiza propiedades del CRM y pipelines por marca/concesionario",
        "appDisplayName": "SIMPA-Application",
        "actionCardContent": "Configuración para {{marca}} / {{dealer}}",
        "inputFieldLabels": {
          "marca": "Marca",
          "modelo": "Modelo",
          "dealer": "Concesionario",
          "razon_social": "Razón Social",
          "tipo_proceso": "Tipo de Proceso"
        },
        "inputFieldDescriptions": {
          "marca": "Marca que determina la configuración de propiedades. Requerida para 'Crear/Actualizar Propiedades de Modelos', 'Crear Todas las Propiedades' y 'Crear/Configurar Pipeline de Negocio'.",
          "modelo": "Modelo a agregar/actualizar en las propiedades. Solo requerido para el proceso 'Crear/Actualizar Propiedades de Modelos'.",
          "dealer": "Concesionario que determina la configuración de propiedades. Requerido para 'Crear/Actualizar Propiedades de Concesionarios' y 'Crear/Configurar Pipeline de Negocio'. Opcional para 'Crear Todas las Propiedades'.",
          "razon_social": "Razón social configurada con un secreto para resolver tokens automáticamente. Requerida para todos los tipos de proceso EXCEPTO 'Crear Todas las Propiedades (Global)'. Dejar vacío para actualización masiva de modelos o proceso global.",
          "tipo_proceso": "Selecciona qué se debe configurar"
        },
        "outputFieldLabels": {
          "success": "Éxito",
          "message": "Mensaje",
          "razonSocial": "Razón Social",
          "propertiesCreated": "Propiedades Creadas",
          "propertiesSkipped": "Propiedades Omitidas",
          "propertiesUpdated": "Propiedades Actualizadas",
          "optionsAdded": "Opciones Agregadas",
          "tipoProceso": "Tipo de Proceso Ejecutado",
          "updatedPropertiesList": "Propiedades Actualizadas",
          "optionDetails": "Detalle de Opciones",
          "pipelineName": "Nombre del Pipeline",
          "pipelineId": "ID del Pipeline",
          "pipelineStagesCount": "Cantidad de Etapas del Pipeline",
          "bulkTotal": "Total Bulk",
          "bulkProcesadas": "Procesadas Bulk",
          "bulkExitosas": "Exitosas Bulk",
          "bulkFallidas": "Fallidas Bulk",
          "bulkErroresCount": "Cantidad de Errores Bulk",
          "bulkErroresDetalle": "Detalle de Errores Bulk",
          "bulkExitosasDetalle": "Detalle de Exitosas Bulk"
        }
